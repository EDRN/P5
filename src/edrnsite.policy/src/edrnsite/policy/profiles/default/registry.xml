<?xml version='1.0' encoding='UTF-8'?>
<registry>
    <record name='plone.site_title' field='site_title'
        interface='Products.CMFPlone.interfaces.controlpanel.ISiteSchema'>
        <field type='plone.registry.field.TextLine'>
            <default>Plone site</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone'
                ns0:translate=''>This shows up in the title bar of browsers and in syndication feeds.</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Site title</title>
        </field>
        <value>Early Detection Research Network</value>
    </record>

    <record name='plone.mark_special_links' interface='Products.CMFPlone.interfaces.controlpanel.ILinkSchema'
        field='mark_special_links'>
        <field type='plone.registry.field.Bool'>
            <default>True</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Marks external or special protocol links with class.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Mark special links</title>
        </field>
        <value>False</value>
    </record>

    <record name='plone.smtp_host' interface='Products.CMFPlone.interfaces.controlpanel.IMailSchema'
        field='smtp_host'>
        <field type='plone.registry.field.TextLine'>
            <default>localhost</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone'
                ns0:translate='help_smtp_server'>The address of your local SMTP (outgoing e-mail) server. Usually 'localhost', unless you use an external server to send e-mail.</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone'
                ns0:translate='label_smtp_server'>SMTP server</title>
        </field>
        <value>non.exist.ent</value>
    </record>

    <record name='plone.email_from_name' interface='Products.CMFPlone.interfaces.controlpanel.IMailSchema'
        field='email_from_name'>
        <field type='plone.registry.field.TextLine'>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Plone generates e-mail using this name as the e-mail sender.</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Site 'From' name</title>
        </field>
        <value>EDRN Portal Admin</value>
    </record>

    <record name='plone.email_from_address' interface='Products.CMFPlone.interfaces.controlpanel.IMailSchema'
        field='email_from_address'>
        <field type='plone.registry.field.ASCIILine'>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Plone generates e-mail using this address as the e-mail return address. It is also used as the destination address for the site-wide contact form and the 'Send test e-mail' feature.</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Site 'From' address</title>
        </field>
        <value>sean.kelly@jpl.nasa.gov</value>
    </record>

    <record name='plone.generate_tabs' interface='Products.CMFPlone.interfaces.controlpanel.INavigationSchema'
        field='generate_tabs'>
        <field type='plone.registry.field.Bool'>
            <default>True</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>By default, all items created at the root level will appear as tabs. You can turn this off if you prefer manually constructing this part of the navigation.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Automatically generate tabs</title>
        </field>
        <value>True</value>
    </record>

    <record name='plone.filter_on_workflow'
        interface='Products.CMFPlone.interfaces.controlpanel.INavigationSchema' field='filter_on_workflow'>
        <field type='plone.registry.field.Bool'>
            <default>False</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>The workflow states that should be shown in the navigation and the site map.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Filter on workflow state</title>
        </field>
        <value>True</value>
    </record>

    <record name='plone.workflow_states_to_show'
        interface='Products.CMFPlone.interfaces.controlpanel.INavigationSchema' field='workflow_states_to_show'>
        <field type='plone.registry.field.Tuple'>
            <default/>
            <missing_value/>
            <required>False</required>
            <value_type type='plone.registry.field.Choice'>
            <vocabulary>plone.app.vocabularies.WorkflowStates</vocabulary>
            </value_type>
        </field>
        <value>
            <element>published</element>
        </value>
    </record>

    <record name='plone.exposeDCMetaTags' interface='Products.CMFPlone.interfaces.controlpanel.ISiteSchema'
        field='exposeDCMetaTags'>
        <field type='plone.registry.field.Bool'>
            <default>False</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Exposes the Dublin Core properties as metatags.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Expose Dublin Core metadata</title>
        </field>
        <value>True</value>
    </record>

    <record name='plone.enable_sitemap' interface='Products.CMFPlone.interfaces.controlpanel.ISiteSchema'
        field='enable_sitemap'>
        <field type='plone.registry.field.Bool'>
            <default>False</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Exposes your content as a file according to the &lt;a href='http://sitemaps.org'&gt;sitemaps.org&lt;/a&gt; standard. You can submit this to compliant search engines like Google, Yahoo and Microsoft. It allows these search engines to more intelligently crawl your site.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Expose sitemap.xml.gz</title>
        </field>
        <value>True</value>
    </record>

    <record name='Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings.search_rss_enabled'
        interface='Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings' field='search_rss_enabled'>
        <field type='plone.registry.field.Bool'>
            <default>True</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Allows users to subscribe to feeds of search results</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Search RSS enabled</title>
        </field>
        <value>False</value>
    </record>

    <record name='Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings.show_author_info'
        interface='Products.CMFPlone.interfaces.syndication.ISiteSyndicationSettings' field='show_author_info'>
        <field type='plone.registry.field.Bool'>
            <default>True</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Should feeds include author information</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Show author info</title>
        </field>
        <value>False</value>
    </record>

    <record name="plone.nonfolderish_tabs"
        interface="Products.CMFPlone.interfaces.controlpanel.INavigationSchema" field="nonfolderish_tabs">
        <field type="plone.registry.field.Bool">
            <default>True</default>
            <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">By default, any content item in the root of the portal will appear as a tab. If you turn this option off, only folders will be shown. This only has an effect if 'automatically generate tabs' is enabled.</description>
            <required>False</required>
            <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Generate tabs for items other than folders.</title>
        </field>
        <value>False</value>
    </record>

    <record name="plone.displayed_types" interface="Products.CMFPlone.interfaces.controlpanel.INavigationSchema"
        field="displayed_types">
        <field type="plone.registry.field.Tuple">
            <default>
                <element>Image</element>
                <element>File</element>
                <element>Link</element>
                <element>News Item</element>
                <element>Folder</element>
                <element>Document</element>
                <element>Event</element>
            </default>
            <description xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">The content types that should be shown in the navigation and site map.</description>
            <missing_value/>
            <required>False</required>
            <title xmlns:ns0="http://xml.zope.org/namespaces/i18n" ns0:domain="plone" ns0:translate="">Displayed content types</title>
            <value_type type="plone.registry.field.Choice">
            <vocabulary>plone.app.vocabularies.ReallyUserFriendlyTypes</vocabulary>
            </value_type>
        </field>
        <value purge='False'>
            <element>eke.knowledge.bodysystemfolder</element>
            <element>eke.knowledge.diseasefolder</element>
            <element>eke.knowledge.publicationfolder</element>
        </value>
    </record>

</registry>
