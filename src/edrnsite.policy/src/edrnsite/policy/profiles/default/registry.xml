<?xml version='1.0' encoding='UTF-8'?>
<registry>
    <record name='plone.site_title' field='site_title'
        interface='Products.CMFPlone.interfaces.controlpanel.ISiteSchema'>
        <field type='plone.registry.field.TextLine'>
            <default>Plone site</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>This shows up in the title bar of browsers and in syndication feeds.</description>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Site title</title>
        </field>
        <value>Early Detection Research Network</value>
    </record>
    <record name='plone.mark_special_links' interface='Products.CMFPlone.interfaces.controlpanel.ILinkSchema'
        field='mark_special_links'>
        <field type='plone.registry.field.Bool'>
            <default>True</default>
            <description xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Marks external or special protocol links with class.</description>
            <required>False</required>
            <title xmlns:ns0='http://xml.zope.org/namespaces/i18n' ns0:domain='plone' ns0:translate=''>Mark special links</title>
        </field>
        <value>False</value>
    </record>
</registry>
