<html
    i18n:domain='eke.knowledge'
    lang='en'
    metal:use-macro='context/main_template/macros/master'
    xml:lang='en'
    xmlns:i18n='http://xml.zope.org/namespaces/i18n'
    xmlns:metal='http://xml.zope.org/namespaces/metal'
    xmlns:tal='http://xml.zope.org/namespaces/tal'
    xmlns='http://www.w3.org/1999/xhtml'>
    <head>
        <title>Dataset (Debug) View</title>
    </head>
    <body>
        <metal:content-core fill-slot='content-core'>
            <metal:content-core define-macro='content-core'>

                <div tal:define='
                    piRV python:context.investigator;
                    wtf python:repr(dir(piRV));
                    toPath python:piRV.to_path;
                '>
                    <label for='investigator'>Investigator:</label>
                    <pre tal:content='toPath'>wtf</pre>
                </div>

            </metal:content-core>
        </metal:content-core>
    </body>
</html>
